{
    "name": "kba-team/cakephp-graylog",
    "description": "Graylog engine for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "cakephp/cakephp": "~4.0",
        "graylog2/gelf-php": "^1.6",
        "kba-team/php-backtrace": "^1.0",
        "kba-team/graylog-utilities": "^1.0"
    },
    "extra": {
        "installer-name": "Graylog"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "kbATeam\\CakePhpGraylog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\kbATeam\\CakePhpGraylog\\": "tests/"
        }
    }
}
